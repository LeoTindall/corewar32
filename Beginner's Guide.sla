            <ITEXT CH="; Semicolon starts a comment"/>
            <para PARENT="Text"/>
            <ITEXT CH="; name, author, and strategy are just metadata."/>
            <para PARENT="Text"/>
            <ITEXT CH="; assert comments let the game ensure that warriors that won't work, aren't executed."/>
            <para PARENT="Text"/>
            <ITEXT FONTSIZE="9" CH=";name Dwarf"/>
            <ITEXT CH="     "/>
            <para PARENT="Text"/>
            <ITEXT FONTSIZE="9" CH=";author A.K. Dewdney"/>
            <para PARENT="Text"/>
            <ITEXT FONTSIZE="9" CH=";strategy Bombs the core at regular intervals."/>
            <para PARENT="Text"/>
            <ITEXT FONTSIZE="9" CH=";assert CORESIZE % 4 == 0"/>
            <para PARENT="Text"/>
            <para PARENT="Text"/>
            <ITEXT FONTSIZE="9" CH="org "/>
            <ITEXT FONTSIZE="9" FCOLOR="Blue" FSHADE="100" CH="loop"/>
