            <para PARENT="Text"/>
            <ITEXT FONTSIZE="9" FCOLOR="Blue" FSHADE="100" CH="loop"/>
            <ITEXT FONTSIZE="9" CH=":   add.ab  #4, "/>
            <ITEXT FONTSIZE="9" FCOLOR="Red" FSHADE="100" CH="bomb"/>
            <ITEXT CH="  ; labels get replaced by their location "/>
            <para PARENT="Text"/>
            <ITEXT CH="                                ; this is add.ab #4, 3"/>
            <para PARENT="Text"/>
            <ITEXT FONTSIZE="9" CH="          mov.i   "/>
            <ITEXT FONTSIZE="9" FCOLOR="Red" FSHADE="100" CH="bomb"/>
            <ITEXT FONTSIZE="9" CH=", @"/>
            <ITEXT FONTSIZE="9" FCOLOR="Red" FSHADE="100" CH="bomb"/>
            <ITEXT CH=" ; move the whole bomb instruction (.i) "/>
            <para PARENT="Text"/>
            <ITEXT CH="                                   ; to @bomb (the location pointed at by the B-field of bomb)"/>
            <para PARENT="Text"/>
            <ITEXT FONTSIZE="9" CH="          jmp     "/>
            <ITEXT FONTSIZE="9" FCOLOR="Blue" FSHADE="100" CH="loop"/>
            <para PARENT="Text"/>
            <ITEXT FONTSIZE="9" FCOLOR="Red" FSHADE="100" CH="bomb"/>
            <ITEXT FONTSIZE="9" CH=":   dat     #0, #0"/>
